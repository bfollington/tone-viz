{
  "name": "fxhash-export",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "clean": "npx rimraf ./dist && npx rimraf ./fxhash-release.zip",
    "rewrite": "mv ./dist/index.html ./dist/index.old.html && sed 's/type=\"module\"//g' ./dist/index.old.html > ./dist/index.html && rm ./dist/index.old.html",
    "zip": "cd dist && npx bestzip ../fxhash-release.zip * && cd ..",
    "release": "npm run clean && npm run build && npm run rewrite && npm run zip"
  },
  "dependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "gallery": "0.0.1"
  },
  "devDependencies": {
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0",
    "@vitejs/plugin-react": "^1.3.0",
    "typescript": "^4.6.3",
    "vite": "^2.9.9"
  }
}