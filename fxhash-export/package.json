{
  "name": "fxhash-export",
  "version": "0.0.0",
  "homepage": "./",
  "private": true,
  "dependencies": {
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "react": "^17.0.0",
    "react-dom": "^17.0.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.6.3",
    "web-vitals": "^1.0.1",
    "gallery": "0.0.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "GENERATE_SOURCEMAP=false react-scripts build",
    "build:lib": "cd .. && cd gallery && yarn build:lib && cd .. && cd fxhash-export",
    "clean": "npx rimraf ./build && npx rimraf ./fxhash-release.zip",
    "zip": "cd build && npx bestzip ../fxhash-release.zip * && cd ..",
    "release": "npm run clean && npm run build:lib ; npm run build && npm run zip",
    "test": "react-scripts test",
    "eject": "react-scripts eject"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/three": "^0.134.0"
  }
}
